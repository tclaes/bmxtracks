{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": "b3a70b53-2c1c-407f-a3a1-11fa45a439c2"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "bmxtracks",
  "projects": {
    "bmxtracks": {
      "projectType": "application",
      "root": "apps/bmxtracks",
      "sourceRoot": "apps/bmxtracks/src",
      "prefix": "bmxtracks",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/bmxtracks",
            "index": "apps/bmxtracks/src/index.html",
            "main": "apps/bmxtracks/src/main.ts",
            "polyfills": "apps/bmxtracks/src/polyfills.ts",
            "tsConfig": "apps/bmxtracks/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/bmxtracks/src/favicon.ico",
              "apps/bmxtracks/src/assets"
            ],
            "styles": [
              "apps/bmxtracks/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/bmxtracks/src/environments/environment.ts",
                  "with": "apps/bmxtracks/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "bmxtracks:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "bmxtracks:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "bmxtracks:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/bmxtracks/src/**/*.ts",
              "apps/bmxtracks/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/bmxtracks"
          ],
          "options": {
            "jestConfig": "apps/bmxtracks/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "bmxtracks-e2e": {
      "root": "apps/bmxtracks-e2e",
      "sourceRoot": "apps/bmxtracks-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/bmxtracks-e2e/cypress.json",
            "tsConfig": "apps/bmxtracks-e2e/tsconfig.e2e.json",
            "devServerTarget": "bmxtracks:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "bmxtracks:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/bmxtracks-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  }
}