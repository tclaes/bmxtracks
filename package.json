    "@types/node": "^14.14.22",
    "babel-loader": "^8.2.2",
    "codelyzer": "^6.0.1",
    "git-format-staged": "^2.1.1",
    "husky": "^4.3.8",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
