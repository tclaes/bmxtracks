    "logrocket": "^1.0.14",
    "normalize.css": "^8.0.1",
    "prettier": "^2.2.1",
    "prismic-dom": "^2.2.4",
    "prismic-javascript": "^3.0.2",
    "rxjs": "~6.6.3",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.3"
